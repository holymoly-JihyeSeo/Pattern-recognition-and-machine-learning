   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N=500\n",
    "mean = [0, 0]\n",
    "std_x =1\n",
    "std_y =1\n",
    "co = 0.5\n",
    "cov = co*std_x*std_y\n",
    "cov_matrix = [[std_x**2,cov],[cov,std_y**2]]\n",
    "\n",
    "x,y = np.random.multivariate_normal(mean, cov_matrix, N).T\n",
    "\n",
    "plt.plot(x,y, 'o')\n",
    "plt.axis([-5, 5, -5 ,5])\n",
    "plt.show()"
   ]
